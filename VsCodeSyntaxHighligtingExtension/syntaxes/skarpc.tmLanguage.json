{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "SkarpC",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.skarpc",
				"match": "\\b(bruk|ope|lukka|låst|gruppe|tom|dersom|elles|for kvar|i|medan|gjør|returner|sann|usann|statisk|prøv|fang|Unntak|Konsoll|Skriv|SkrivLinje|Les|LesLinje|variabel|heltall|tekst|LeggTil|konstant|Hovedfunksjon|etter kvart|avvent|avbryt|Oppgave|ErTom|ny)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.skarpc",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.skarpc",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.skarpc"
}